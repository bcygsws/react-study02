{
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": false
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		[
			"import",
			{
				"libraryName": "antd",
				"libraryDirectory": "es",
				"style": "css"
			}
		],
		"@babel/transform-runtime",
		["@babel/plugin-proposal-class-properties", { "loose": false }]
	]
}
